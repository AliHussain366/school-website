/* Global Styles */
body {
    margin: 0;
    font-family: 'Arial', sans-serif;
    line-height: 1.6;
    color: #333;
    background: linear-gradient(135deg, #f3f4f7, #ffffff);
}

.container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 20px;
}

/* Navigation Bar */
.navbar {
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: linear-gradient(90deg, #007bff, #00d4ff);
    padding: 10px 20px;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
}

.navbar .logo h1 {
    color: white;
    font-size: 1.8rem;
    text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.3);
}

.navbar ul {
    list-style: none;
    margin: 0;
    padding: 0;
    display: flex;
}

.navbar ul li {
    margin: 0 10px;
}

.navbar ul li a {
    text-decoration: none;
    color: white;
    padding: 10px 15px;
    border-radius: 5px;
    transition: background 0.3s ease;
}

.navbar ul li a:hover {
    background: rgba(255, 255, 255, 0.2);
}

/* Hero Section */
.hero {
    background: linear-gradient(135deg, #007bff, #00d4ff);
    color: white;
    text-align: center;
    padding: 100px 20px;
}

.hero-content h2 {
    font-size: 2.5rem;
    margin-bottom: 20px;
    animation: fadeIn 2s ease-in-out;
}

.hero-content p {
    font-size: 1.2rem;
    margin-bottom: 20px;
}

.hero-content .cta-button {
    display: inline-block;
    background: #ffdd57;
    color: #333;
    padding: 10px 20px;
    border-radius: 5px;
    text-decoration: none;
    font-weight: bold;
    transition: transform 0.3s ease, background 0.3s ease;
}

.hero-content .cta-button:hover {
    background: #ffc107;
    transform: scale(1.1);
}

/* Section Headings */
section h2 {
    text-align: center;
    font-size: 2rem;
    margin-bottom: 20px;
    color: #007bff;
    text-shadow: 1px 1px 5px rgba(0, 0, 0, 0.1);
}

/* About Section */
.about, .admissions, .academics, .events {
    background: white;
    margin: 20px 0;
    border-radius: 8px;
    padding: 20px;
    box-shadow: 0 5px 10px rgba(0, 0, 0, 0.1);
    animation: fadeInUp 1.5s ease;
}

/* Management Section */
.management {
    background: white;
    margin: 20px 0;
    border-radius: 8px;
    padding: 20px;
    box-shadow: 0 5px 10px rgba(0, 0, 0, 0.1);
}

.management-feature {
    margin-bottom: 20px;
}

.management-feature h3 {
    color: #333;
    margin-bottom: 10px;
}

.management-feature form input,
.management-feature form textarea,
.management-feature form button {
    width: 100%;
    margin: 10px 0;
    padding: 10px;
    font-size: 1rem;
    border: 1px solid #ccc;
    border-radius: 5px;
}

.management-feature form button {
    background: #007bff;
    color: white;
    font-weight: bold;
    transition: background 0.3s ease, transform 0.3s ease;
}

.management-feature form button:hover {
    background: #0056b3;
    transform: scale(1.05);
}

/* Contact Section */
.contact form input, .contact form textarea {
    width: 100%;
    margin: 10px 0;
    padding: 10px;
    font-size: 1rem;
    border: 1px solid #ccc;
    border-radius: 5px;
}

.contact form button {
    background: #28a745;
    color: white;
    padding: 10px 20px;
    border: none;
    border-radius: 5px;
    font-size: 1rem;
    cursor: pointer;
    transition: background 0.3s ease, transform 0.3s ease;
}

.contact form button:hover {
    background: #218838;
    transform: scale(1.05);
}

/* Footer */
footer {
    background: #333;
    color: white;
    text-align: center;
    padding: 10px 0;
    margin-top: 20px;
}

footer p {
    margin: 0;
}

/* Animations */
@keyframes fadeIn {
    from {
        opacity: 0;
    }
    to {
        opacity: 1;
    }
}

@keyframes fadeInUp {
    from {
        opacity: 0;
        transform: translateY(20px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

/* Responsive Design */
@media (max-width: 768px) {
    .navbar ul {
        flex-direction: column;
        align-items: center;
    }

    .navbar ul li {
        margin: 5px 0;
    }

    .hero-content h2 {
        font-size: 2rem;
    }

    section h2 {
        font-size: 1.5rem;
    }
}
/* Global Styles */
*,
*::before,
*::after {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

body {
    font-family: 'Roboto', sans-serif;
    line-height: 1.6;
    color: #333;
    background: #f7f7f7;
    overflow-x: hidden;
}

/* Basic Container */
.container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 20px;
}

/* Navigation Bar */
.navbar {
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: #003366;
    padding: 15px 30px;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
    position: sticky;
    top: 0;
    z-index: 100;
}

.navbar h1 {
    color: white;
    font-size: 2rem;
    font-weight: 600;
}

.navbar ul {
    list-style: none;
    display: flex;
}

.navbar ul li {
    margin: 0 15px;
}

.navbar ul li a {
    text-decoration: none;
    color: white;
    padding: 10px 20px;
    font-size: 1rem;
    font-weight: 500;
    border-radius: 5px;
    transition: all 0.3s ease-in-out;
}

.navbar ul li a:hover {
    background-color: #ff6600;
    transform: translateY(-3px);
}

/* Hero Section */
.hero {
    background: linear-gradient(45deg, #1a2a6c, #b21f1f, #fbb034);
    color: white;
    text-align: center;
    padding: 100px 20px;
    position: relative;
    overflow: hidden;
    border-radius: 8px;
    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
}

.hero h2 {
    font-size: 3rem;
    margin-bottom: 20px;
    text-transform: uppercase;
    font-weight: 600;
    animation: fadeInUp 1.5s ease-out;
}

.hero p {
    font-size: 1.2rem;
    margin-bottom: 30px;
    animation: fadeInUp 2s ease-out;
}

.hero .cta-button {
    font-size: 1.1rem;
    padding: 12px 30px;
    background-color: #ff6600;
    color: white;
    border: none;
    border-radius: 30px;
    text-transform: uppercase;
    cursor: pointer;
    transition: all 0.3s ease-in-out;
    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);
}

.hero .cta-button:hover {
    background-color: #ff3300;
    transform: scale(1.1);
}

@keyframes fadeInUp {
    0% {
        opacity: 0;
        transform: translateY(20px);
    }
    100% {
        opacity: 1;
        transform: translateY(0);
    }
}

/* Section Headings */
section h2 {
    font-size: 2rem;
    text-align: center;
    color: #003366;
    margin-bottom: 30px;
    text-transform: uppercase;
    letter-spacing: 1px;
    font-weight: 600;
}

/* Form Elements */
input[type="text"], input[type="number"], input[type="date"], select, textarea {
    width: 100%;
    padding: 12px 15px;
    font-size: 1rem;
    border: 2px solid #ddd;
    border-radius: 8px;
    margin: 10px 0 20px;
    background-color: #fff;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
    transition: all 0.3s ease;
}

input[type="text"]:focus, input[type="number"]:focus, input[type="date"]:focus, select:focus, textarea:focus {
    border-color: #ff6600;
    box-shadow: 0 0 10px rgba(255, 102, 0, 0.5);
}

/* Button Styles */
button {
    padding: 12px 25px;
    font-size: 1rem;
    background-color: #003366;
    color: white;
    border: none;
    border-radius: 8px;
    cursor: pointer;
    font-weight: 600;
    transition: all 0.3s ease;
}

button:hover {
    background-color: #ff6600;
    transform: translateY(-3px);
}

/* Table Styles */
table {
    width: 100%;
    border-collapse: collapse;
    margin-bottom: 30px;
}

table th, table td {
    padding: 12px;
    text-align: left;
    font-size: 1rem;
    border-bottom: 2px solid #f2f2f2;
}

table th {
    background-color: #003366;
    color: white;
    font-weight: 600;
}

table td {
    background-color: #fff;
    color: #555;
}

/* Management Section */
.management {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 30px;
}

.management .management-feature {
    background-color: white;
    padding: 30px;
    border-radius: 8px;
    box-shadow: 0 5px 10px rgba(0, 0, 0, 0.1);
}

.management .management-feature h3 {
    font-size: 1.5rem;
    margin-bottom: 15px;
    color: #003366;
    font-weight: 600;
}

/* Footer */
footer {
    background-color: #003366;
    color: white;
    text-align: center;
    padding: 15px;
    margin-top: 50px;
}

footer p {
    font-size: 1rem;
}

/* Responsive Design */
@media (max-width: 768px) {
    .navbar ul {
        flex-direction: column;
        align-items: center;
    }

    .navbar ul li {
        margin: 10px 0;
    }

    .hero h2 {
        font-size: 2.5rem;
    }

    .hero p {
        font-size: 1.1rem;
    }

    .management {
        grid-template-columns: 1fr;
    }

    .management .management-feature {
        margin-bottom: 30px;
    }

    section h2 {
        font-size: 1.6rem;
    }
}

@media (max-width: 480px) {
    .hero h2 {
        font-size: 2rem;
    }

    .hero .cta-button {
        font-size: 1rem;
        padding: 10px 25px;
    }
}
